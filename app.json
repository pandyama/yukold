{
  "expo": {
    "name": "yukold",
    "slug": "yukold",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-yukold.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mp.yukold",
      "buildNumber": "1.0.4",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to fetch weather for the area you are located in",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.mp.yukold",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c90f4720-589c-4c49-98d6-244e7e77b2e7"
      }
    }
  }
}
